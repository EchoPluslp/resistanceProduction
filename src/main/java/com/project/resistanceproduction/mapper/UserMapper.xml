<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.resistanceproduction.mapper.UserMapper">

    
    <select id="getUserByNameAndPassword" parameterType="com.project.resistanceproduction.entity.User"  resultType="com.project.resistanceproduction.entity.User">
       select * from user where username = #{user.username} and password = #{user.password}
    </select>
    

</mapper>